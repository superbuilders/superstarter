{
	"name": "superstarter",
	"version": "1.0.0",
	"private": false,
	"type": "module",
	"scripts": {
		"build": "next build",
		"check": "biome check .",
		"check:unsafe": "biome check --write --unsafe .",
		"check:write": "biome check --write .",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"db:drop": "bun run src/db/scripts/drop.ts",
		"dev": "next dev --turbo",
		"dev:inngest": "inngest-cli dev",
		"preview": "next build && next start",
		"start": "next start",
		"typecheck": "tsc --noEmit && bun run check:write",
		"prepare": "husky"
	},
	"dependencies": {
		"@clerk/nextjs": "^6.28.0",
		"@superbuilders/errors": "^2.0.0",
		"@superbuilders/slog": "^3.1.0",
		"@t3-oss/env-nextjs": "^0.13.8",
		"drizzle-orm": "^0.44.2",
		"inngest": "^3.40.0",
		"inngest-cli": "^1.8.2",
		"next": "^15.3.4",
		"postgres": "^3.4.7",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"zod": "^3.25.67"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.0-beta.6",
		"@tailwindcss/postcss": "^4.1.11",
		"@types/node": "^24.0.10",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"drizzle-kit": "^0.31.4",
		"husky": "^9.1.7",
		"postcss": "^8.5.6",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.8.3"
	},
	"ct3aMetadata": {
		"initVersion": "7.39.3"
	},
	"trustedDependencies": [
		"@biomejs/biome",
		"@tailwindcss/oxide",
		"inngest-cli"
	]
