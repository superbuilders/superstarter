{
	"name": "superstarter",
	"version": "1.0.0",
	"private": false,
	"type": "module",
	"scripts": {
		"dev": "bun --bun next dev",
		"dev:inngest": "bun --bun inngest-cli dev --queue-workers 4000 --poll-interval 1",
		"build": "bun --bun next build",
		"start": "bun --bun next start",
		"lint": "bun --bun biome lint --write",
		"lint:biome": "bun --bun biome lint --write",
		"format": "bun --bun biome format --write",
		"typecheck": "bun --bun tsc",
		"db:generate": "bun --bun run scripts/drizzle-kit-shim.ts generate",
		"db:migrate": "bun --bun run scripts/drizzle-kit-shim.ts migrate",
		"db:push": "bun --bun run scripts/drizzle-kit-shim.ts push",
		"db:studio": "bun --bun run scripts/drizzle-kit-shim.ts studio",
		"db:drop": "bun --bun run src/db/scripts/drop.ts"
	},
	"dependencies": {
		"@superbuilders/errors": "^3.0.2",
		"@superbuilders/slog": "^4.0.0",
		"@t3-oss/env-nextjs": "^0.13.8",
		"drizzle-orm": "^0.45.1",
		"inngest": "^3.47.0",
		"next": "^16.0.10",
		"postgres": "^3.4.7",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"zod": "^4.1.13"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@tailwindcss/postcss": "^4.1.18",
		"@types/bun": "^1.3.4",
		"@types/node": "^25.0.1",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"drizzle-kit": "^0.31.8",
		"inngest-cli": "^1.15.0",
		"postcss": "^8.5.6",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3"
	},
	"ct3aMetadata": {
		"initVersion": "7.39.3"
	},
	"trustedDependencies": [
		"@biomejs/biome",
		"@tailwindcss/oxide",
		"inngest-cli"
	]
}
